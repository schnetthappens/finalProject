<div class="user-show-latest-entry">
  <div class="user-show-latest-entry-wrapper">
    <div class="user-show-latest-entry-title">
      <h2 class="primary-subheading user-show-latest-entry-title-text">Thursday</h2>
    </div>
    <ul class="entries-list user-show-entries-list-thursday">
      {{#each model.thursday.day_entries as |thursday|}}
      <li class="entry">
        <div class="entry-wrapper">
          <h2 class="entry-project">{{thursday.project}}</h2>
          <h4 class="entry-client">{{thursday.client}}</h4>
          <h3 class="entry-hours">{{thursday.hours}}</h3>
        </div>
      </li>
      {{/each}}
    </ul>
  </div>
</div>

{{yield}}
